  "main": "electron/main.js",
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.1",
    "@rescripts/cli": "0.0.10",
    "@rescripts/rescript-env": "0.0.10",
    "electron": "^9.4.0",
    "electron-is-dev": "^1.1.0",
    "firebase": "^6.1.0",
    "firebaseui": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
